# - Config file for the @PROJECT_NAME@ package
# It defines the following variables
#  @PROJECT_NAME@_EXECUTABLE   - the @PROJECT_NAME@ executable

list(FIND OverallFoundPackages @PROJECT_NAME@ isPackageAlreadyFound)
if(${isPackageAlreadyFound} GREATER -1)
	return()
endif()
 
# Compute the installation prefix relative to this file.
GET_FILENAME_COMPONENT(_IMPORT_PREFIX "${CMAKE_CURRENT_LIST_FILE}" PATH)
GET_FILENAME_COMPONENT(_IMPORT_PREFIX "${_IMPORT_PREFIX}" PATH)

set(@PROJECT_NAME@_EXECUTABLE ${_IMPORT_PREFIX}/bin/@INSTALL_EXECUTABLE@)
if(DEFINED INSTALL_TEST_EXECUTABLE)
	set(@PROJECT_NAME@_TEST_EXECUTABLE ${_IMPORT_PREFIX}/bin/@INSTALL_TEST_EXECUTABLE@)
endif()	

# Our package dependencies (contains definitions for IMPORTED targets)
include("${_IMPORT_PREFIX}/config/@PROJECT_NAME@Targets.cmake")


